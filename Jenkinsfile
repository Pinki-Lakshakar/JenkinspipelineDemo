pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'building'
            }
        }
         stage('Deploy') {
            steps {
                echo 'deployment'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing here'
            }
        }
    }
}
